@model List<LoanManagement.Entity.Product>
@{
}

<h1>Products</h1>

<table class="table">
    <thead>
        <tr>
            <td>Code</td>
            <td>Name</td>
            <td>Description</td>
            <td>InterestRate</td>
            <td>TermInDays</td>
            <th><a href="Products/Add"><h2>Add</h2></a></th>
        </tr>
    </thead>
    <tbody>
        @foreach(var emp in Model)
        {
            <tr>
                <td>@emp.Code</td>
                <td>@emp.Name</td>
                <td>@emp.Description</td>
                <td>@emp.InterestRate</td>
                <td>@emp.TermInDays</td>
                <td><a href="Products/View/@emp.Id">View</a></td>
            </tr>
        }
    </tbody>
</table>
